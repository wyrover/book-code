// cerrno standard header
#pragma once
#ifndef _CERRNO_
#define _CERRNO_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <errno.h>
 #define _STD_USING

 #ifndef errno
  #define errno	errno
  #endif /* errno */

#else /* _STD_USING */
 #include <errno.h>

#ifndef RC_INVOKED
 #if _GLOBAL_USING
_STD_BEGIN
  #ifndef errno
   #define errno	errno
using ::errno;
  #endif /* errno */

_STD_END
 #endif /* _GLOBAL_USING */
#endif /* RC_INVOKED */

#endif /* _CERRNO_ */
#endif /* _STD_USING */

