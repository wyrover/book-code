// cstdarg standard header
#pragma once
#ifndef _CSTDARG_
#define _CSTDARG_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <stdarg.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <stdarg.h>

#ifndef RC_INVOKED
 #if _GLOBAL_USING
_STD_BEGIN
using ::va_list;
_STD_END
 #endif /* _GLOBAL_USING */
#endif /* RC_INVOKED */

#endif /* _STD_USING */
#endif /* _CSTDARG_ */

